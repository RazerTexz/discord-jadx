<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="Base.Theme.AppCompat" parent="@style/Base.V26.Theme.AppCompat">
    </style>
    <style name="Base.Theme.AppCompat.Light" parent="@style/Base.V26.Theme.AppCompat.Light">
    </style>
    <style name="Base.V26.Theme.AppCompat" parent="@style/Base.V23.Theme.AppCompat">
        <item name="colorError">?android:attr/colorError</item>
    </style>
    <style name="Base.V26.Theme.AppCompat.Light" parent="@style/Base.V23.Theme.AppCompat.Light">
        <item name="colorError">?android:attr/colorError</item>
    </style>
    <style name="Base.V26.Widget.AppCompat.Toolbar" parent="@style/Base.V7.Widget.AppCompat.Toolbar">
        <item name="android:touchscreenBlocksFocus">true</item>
        <item name="android:keyboardNavigationCluster">true</item>
    </style>
    <style name="Base.Widget.AppCompat.Toolbar" parent="@style/Base.V26.Widget.AppCompat.Toolbar">
    </style>
    <style name="UiKit.TextInputLayout.EditText.SingleLine.CountryCode" parent="@style/UiKit.TextInputLayout.EditText.SingleLine">
        <item name="android:maxLength">6</item>
        <item name="android:inputType">0x2</item>
        <item name="android:autofillHints">phoneCountryCode</item>
    </style>
    <style name="UiKit.TextInputLayout.EditText.SingleLine.Email" parent="@style/UiKit.TextInputLayout.EditText.SingleLine">
        <item name="android:inputType">0x21</item>
        <item name="android:autofillHints">emailAddress</item>
    </style>
    <style name="UiKit.TextInputLayout.EditText.SingleLine.Password" parent="@style/UiKit.TextInputLayout.EditText.SingleLine">
        <item name="android:inputType">0x81</item>
        <item name="android:autofillHints">password</item>
    </style>
    <style name="UiKit.TextInputLayout.EditText.SingleLine.Phone" parent="@style/UiKit.TextInputLayout.EditText.SingleLine">
        <item name="android:maxLength">18</item>
        <item name="android:inputType">0x3</item>
        <item name="android:autofillHints">phoneNumber</item>
    </style>
    <style name="UiKit.TextInputLayout.EditText.SingleLine.Search" parent="@style/UiKit.TextInputLayout.EditText.SingleLine">
        <item name="android:padding">0dp</item>
        <item name="android:hint">@string/search</item>
        <item name="android:imeOptions">0x6</item>
        <item name="android:importantForAutofill">0x2</item>
    </style>
    <style name="UiKit.TextInputLayout.EditText.SingleLine.Username" parent="@style/UiKit.TextInputLayout.EditText.SingleLine">
        <item name="android:autofillHints">username</item>
    </style>
    <style name="Widget.Design.AppBarLayout" parent="@android:style/Widget">
        <item name="android:background">?attr/colorPrimary</item>
        <item name="android:stateListAnimator">@animator/design_appbar_state_list_animator</item>
        <item name="android:touchscreenBlocksFocus">true</item>
        <item name="android:keyboardNavigationCluster">true</item>
    </style>
</resources>

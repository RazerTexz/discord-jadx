<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android" style="@style/AppAlertDialog">
    <TextView
        android:text="@string/user_dm_settings"
        style="@style/AppAlertDialogHeader"/>
    <LinearLayout style="@style/AppAlertDialogContent">
        <TextView
            android:text="@string/user_dm_settings_question"
            style="@style/AppAlertDialogText"/>
    </LinearLayout>
    <LinearLayout style="@style/AppAlertDialogFooter">
        <com.google.android.material.button.MaterialButton
            android:id="@+id/settings_privacy_defaults_existing_no"
            android:text="@string/no_text"
            style="@style/UiKit.Material.Button.Transparent.Fit"/>
        <com.google.android.material.button.MaterialButton
            android:id="@+id/settings_privacy_defaults_existing_yes"
            android:text="@string/yes_text"
            style="@style/UiKit.Material.Button.Red.Fit"/>
    </LinearLayout>
</LinearLayout>

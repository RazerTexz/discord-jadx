<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android">
    <TextView
        android:textSize="12dp"
        android:textColor="?attr/colorChannelsDefault"
        android:id="@+id/voice_user_limit_current"
        android:background="@drawable/voice_user_limit_view_left_bg"
        android:paddingLeft="8dp"
        android:paddingTop="2.25dp"
        android:paddingRight="2dp"
        android:paddingBottom="2.25dp"
        android:includeFontPadding="false"
        android:drawablePadding="4dp"
        android:backgroundTint="?attr/colorBackgroundMobilePrimary"
        style="@style/UiKit.TextView"/>
    <TextView
        android:textSize="@dimen/uikit_textsize_small"
        android:textColor="?attr/colorChannelsDefault"
        android:id="@+id/voice_user_limit_max"
        android:background="@drawable/voice_user_limit_view_right_bg"
        android:paddingLeft="8dp"
        android:paddingTop="2.25dp"
        android:paddingRight="8dp"
        android:paddingBottom="2.25dp"
        android:includeFontPadding="false"
        android:backgroundTint="?attr/colorBackgroundTertiary"
        style="@style/UiKit.TextView"/>
</merge>
